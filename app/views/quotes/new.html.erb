<h1>new</h1>
<div class="container">
  <div class="row">
    <div class="col-md-8 col-md-offset-2">
      <h1>Add a new quote</h1>

      <%= simple_form_for @quote do |f| %>
        <div class="form-inputs">
        <%= f.input :content %>
      </div>
        <div class="form-actions">
        <%= f.button :submit, class: 'btn btn-primary' %>
      </div>
      <% end %>
    </div>
  </div>
</div>
